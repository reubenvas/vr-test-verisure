{
    "name": "javascript-development-environment",
    "version": "1.0.0",
    "description": "JavaScript development environment Pluralsight course by Cory House",
    "scripts": {
        "bab": "nodemon --help",
        "lint": "esw . --ext .js,.ts,.tsx  --color",
        "lint:watch": "npm run lint -- --watch",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch",
        "test": "jest --reporter=progress",
        "test:watch": "npm test -- --watch",
        "app:dev": "cross-env NODE_ENV=development  nodemon --exec babel-node --extensions .ts,.js server/src",
        "startMessage": "babel-node buildScripts/startMessage.js",
        "prestart:dev": "npm run startMessage",
        "start:dev": "npm-run-all --parallel lint:watch type-check:watch test:watch start-mockapi app:dev",
        "start": "npm run start:dev -s",
        "generate-mock-data": "babel-node buildScripts/generateMockData",
        "prestart-mockapi": "npm run generate-mock-data",
        "start-mockapi": "json-server --watch src/api/db.json --port 3001",
        "update-dependancy-versions": "ncu -u || npx npm-check-updates -u",
        "continue": "./scripts/continue.sh",
        "clean-client-dist": "rimraf ./dist && mkdir dist",
        "clean-server-dist": "rimraf ./server/dist && cd server && mkdir dist",
        "prebuild": "npm-run-all type-check lint test clean-client-dist clean-server-dist",
        "build-client": "babel-node --extensions .ts,.js buildScripts/build.js",
        "build-server": "babel --out-dir ./server/dist ./server/src --extensions .js,.ts",
        "build": "cross-env NODE_ENV=production npm-run-all --parallel build-client build-server",
        "app:prod": "cross-env NODE_ENV=production node server/dist",
        "start:prod": "npm run app:prod"
    },
    "author": "Reuben Vas",
    "license": "",
    "dependencies": {
        "@babel/runtime": "^7.8.0",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "whatwg-fetch": "3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.0.0",
        "@babel/core": "^7.7.7",
        "@babel/node": "^7.0.0",
        "@babel/plugin-proposal-optional-chaining": "^7.7.5",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.0",
        "@babel/preset-env": "^7.7.7",
        "@babel/preset-react": "^7.7.4",
        "@babel/preset-typescript": "^7.7.7",
        "@babel/register": "^7.0.0",
        "@types/compression": "^1.0.1",
        "@types/express": "^4.17.2",
        "@types/jest": "^24.0.25",
        "@types/jsdom": "^12.2.4",
        "@types/numeral": "0.0.26",
        "@types/react": "^16.9.17",
        "@types/webpack": "^4.41.2",
        "@types/webpack-dev-middleware": "^2.0.3",
        "@typescript-eslint/eslint-plugin": "^2.13.0",
        "@typescript-eslint/parser": "^2.13.0",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-react": "^1.0.0",
        "chai": "4.2.0",
        "chalk": "3.0.0",
        "cheerio": "0.22.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.4.2",
        "dotenv": "^8.2.0",
        "dotenv-webpack": "^1.7.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "eslint-watch": "6.0.1",
        "faker": "^4.1.0",
        "html-webpack-plugin": "3.2.0",
        "jest": "^24.9.0",
        "jsdom": "15.2.1",
        "json-schema-faker": "0.4.7",
        "json-server": "0.15.1",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "6.2.2",
        "nock": "11.7.0",
        "node-sass": "^4.13.0",
        "nodemon": "^2.0.2",
        "npm-run-all": "4.1.5",
        "numeral": "2.0.6",
        "open": "7.0.0",
        "postcss-loader": "^3.0.0",
        "rimraf": "3.0.0",
        "sass-loader": "^8.0.1",
        "style-loader": "^1.1.1",
        "surge": "0.21.3",
        "terser-webpack-plugin": "^2.3.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-middleware": "3.7.2",
        "webpack-dev-server": "^3.10.1",
        "webpack-hot-middleware": "2.25.0",
        "webpack-md5-hash": "0.0.6"
    }
}
